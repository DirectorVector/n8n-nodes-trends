{
  "name": "n8n-nodes-trends",
  "version": "1.0.0",
  "description": "n8n node for Google Trends data using @alkalisummer/google-trends-js",
  "keywords": [
    "n8n-community-node-package",
    "google-trends",
    "trends",
    "analytics"
  ],
  "license": "MIT",
  "homepage": "https://github.com/DirectorVector/n8n-nodes-trends",
  "author": {
    "name": "DirectorVector"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DirectorVector/n8n-nodes-trends.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/GoogleTrends/GoogleTrends.node.js"
    ]
  },
  "dependencies": {
    "@alkalisummer/google-trends-js": "^0.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
